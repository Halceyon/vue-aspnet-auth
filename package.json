{
  "name": "vue-aspnet-auth",
  "version": "0.0.0-beta",
  "description": "Vue plugin for auth to OWIN MVC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Halceyon/vue-aspnet-auth.git"
  },
  "keywords": [
    "vue",
    "aspnet",
    "auth",
    "owin"
  ],
  "author": "Craig Pretorius <craigp@codehq.co.za>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Halceyon/vue-aspnet-auth/issues"
  },
  "homepage": "https://github.com/Halceyon/vue-aspnet-auth#readme",
  "main": "dist/vue-aspnet-auth.js",
  "jsnext:main": "dist/vue-aspnet-auth.es.js",
  "babel": {
    "presets": [
      "latest",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb-base"
  },
  "dependencies": {
    "aspnet-auth": "^1.0.2",
    "babel-runtime": "^6.11.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "browser-cookies": "^1.2.0",
    "chai": "^4.0.0-canary.1",
    "coveralls": "^3.0.3",
    "del": "^3.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "istanbul": "^1.1.0-alpha.1",
    "json-stable-stringify": "^1.0.1",
    "mocha": "^5.0.4",
    "parse-json": "^4.0.0",
    "rollup": "^2.2.0",
    "rollup-plugin-babel": "^3.0.3",
    "semantic-release": "^15.13.13",
    "sinon": "^6.0.0",
    "sinon-stub-promise": "^4.0.0",
    "travis-deploy-once": "^5.0.0",
    "vue": "^2.5.13"
  },
  "scripts": {
    "build": "node tools/build",
    "test": "mocha --compilers js:babel-register",
    "test:cover": "babel-node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  }
}
